{
  "name": "@blocksuite/indexer",
  "version": "0.12.0",
  "description": "indexer",
  "type": "module",
  "repository": "toeverything/blocksuite",
  "scripts": {
    "build": "tsc",
    "test:unit": "vitest --run",
    "test": "pnpm test:unit"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "fuse.js": "^7.0.0",
    "@datastructures-js/priority-queue": "^6.3.1"
  },
  "devDependencies": {},
  "peerDependencies": {},
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "./impl/*": {
        "types": "./dist/impl/*.d.ts",
        "module": "./dist/impl/*.js",
        "import": "./dist/impl/*.js"
      }
    }
  },
  "files": [
    "src",
    "dist",
    "!src/__tests__",
    "!dist/__tests__"
  ]
}
